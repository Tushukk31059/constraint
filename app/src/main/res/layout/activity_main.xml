<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".10" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".15"
        />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh20"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent=".20"
    />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent=".26 "
        android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent=".42"
        android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".5"
        />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".55"
        />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent=".6 "
        android:orientation="horizontal"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh63"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent=".63 "
        android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gh68"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_percent=".68 "
        android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gv7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".07"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gv19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".19"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gv47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".47"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gv81"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".81"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:id="@+id/gv52"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".52"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:id="@+id/gv58"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".58"
        />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gv25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".25"
        />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/material_dynamic_neutral80"
        android:textColor="@color/black"
        android:text="One Way"
        app:layout_constraintBottom_toBottomOf="@id/gh60"
        app:layout_constraintEnd_toEndOf="@id/gv47"
        app:layout_constraintStart_toStartOf="@id/gv7"
        app:layout_constraintTop_toTopOf="@id/gh55" />

    <TextView
        android:background="@color/material_dynamic_neutral80"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textColor="@color/black"
        android:text="1 Traveller"
        app:layout_constraintBottom_toBottomOf="@id/gh68"
        app:layout_constraintEnd_toEndOf="@id/gv25"
        app:layout_constraintStart_toStartOf="@id/gv7"
        app:layout_constraintTop_toTopOf="@id/gh63"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/gv43"
        app:layout_constraintGuide_percent=".43"
        />
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/gv62"
        app:layout_constraintGuide_percent=".62"/>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/gh10"
        app:layout_constraintStart_toStartOf="@+id/gv19"
        app:layout_constraintEnd_toEndOf="@id/gv43"
        android:background="@mipmap/ic_launcher"
        />
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/gh10"
        app:layout_constraintStart_toStartOf="@+id/gv43"
        app:layout_constraintEnd_toEndOf="@id/gv62"
        android:background="@mipmap/ic_launcher_round"
        />
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/gh10"
        app:layout_constraintStart_toStartOf="@+id/gv62"
        app:layout_constraintEnd_toEndOf="@id/gv81"
        android:background="@mipmap/ic_launcher"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gh10"
        app:layout_constraintBottom_toBottomOf="@+id/gh15"
        app:layout_constraintStart_toStartOf="@id/gv19"
        app:layout_constraintEnd_toEndOf="@id/gv43"
        android:text="space stations"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gh10"
        app:layout_constraintBottom_toBottomOf="@+id/gh15"
        app:layout_constraintStart_toStartOf="@id/gv43"
        app:layout_constraintEnd_toEndOf="@id/gv62"
        android:text="flights"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/gh10"
        app:layout_constraintBottom_toBottomOf="@+id/gh15"
        app:layout_constraintStart_toStartOf="@id/gv62"
        app:layout_constraintEnd_toEndOf="@id/gv81"
        android:text="rovers"
        />
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@+id/gv19"
        app:layout_constraintTop_toTopOf="@id/gh20"
        android:text="DCA"
        android:background="@color/teal_200"
        app:layout_constraintBottom_toBottomOf="@id/gh42"
        app:layout_constraintEnd_toEndOf="@id/gv47"/>
    <androidx.constraintlayout.widget.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".34"
        android:id="@+id/gh34"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="MARS"
        app:layout_constraintStart_toStartOf="@id/gv58"
        app:layout_constraintEnd_toEndOf="@id/gv81"
        app:layout_constraintTop_toTopOf="@id/gh20"
        android:gravity="center"
        android:background="@color/teal_200"
        app:layout_constraintBottom_toBottomOf="@id/gh42"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@mipmap/ic_launcher"
        app:layout_constraintTop_toTopOf="@id/gh26"
        app:layout_constraintBottom_toBottomOf="@id/gh34"
        app:layout_constraintStart_toStartOf="@id/gv43"
        app:layout_constraintEnd_toEndOf="@+id/gv62"
        />
    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/gv47"
        app:layout_constraintTop_toTopOf="@id/gh55"


        />


</androidx.constraintlayout.widget.ConstraintLayout>